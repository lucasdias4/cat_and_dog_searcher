<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/title_start_logo_view"
        style="@style/D1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title_start_logo_view"
        android:textColor="@color/text_primary"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/title_middle_logo_view"
        style="@style/A_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_start_title_middle_logo_view"
        android:layout_marginBottom="@dimen/margin_bottom_title_middle_logo_view"
        android:text="@string/title_middle_logo_view"
        android:textColor="@color/text_quaternary"
        app:layout_constraintBottom_toBottomOf="@id/title_start_logo_view"
        app:layout_constraintStart_toEndOf="@id/title_start_logo_view" />

    <TextView
        android:id="@+id/title_end_logo_view"
        style="@style/D1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_start_title_logo_view"
        android:text="@string/title_end_logo_view"
        android:textColor="@color/text_primary"
        app:layout_constraintBottom_toBottomOf="@id/title_start_logo_view"
        app:layout_constraintStart_toEndOf="@id/title_middle_logo_view" />

</androidx.constraintlayout.widget.ConstraintLayout>